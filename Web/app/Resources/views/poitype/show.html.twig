{% extends 'base.html.twig' %}

{% block body %}

<div id="page-content-wrapper">
    <div class="container-fluid">

        <div class="home_title">
            <p>POI Type</p>
        </div>

        <div id="profile_container">

    <table class="cruds">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ poiType.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ poiType.name }}</td>
            </tr>
            <tr>
                <th>Placesid</th>
                <td>{{ poiType.placesId }}</td>
            </tr>
            <tr>
                <th>Color</th>
                <td>{{ poiType.color }}</td>
            </tr>
            <tr>
                <th>Icon</th>
                <td>{{ poiType.icon }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_poitype_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_poitype_edit', { 'id': poiType.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
        </div>
    </div>
</div>
{% endblock %}
